<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
        res.redirect('/')
    } %>

<%- messages() %>

<% if (errors) { %>
    <ul class="notice">
        <% errors.array().forEach(error => {%>
        <li><%= error.msg %></li>
        <% }) %>
    </ul>
    <% } %>

    <h2>Available Orders</h2>
    <%- availableTable %>
    <% if(locals.accountData.account_type == 'Admin') { %>
    <h2>All Orders</h2>
    <%- allTable %>
    <% } %>